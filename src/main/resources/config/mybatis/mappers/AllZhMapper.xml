<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.silita.biaodaa.dao.AllZhMapper" >

  <select id="getAllZhByName" parameterType="String" resultType="com.silita.biaodaa.model.AllZh">
    SELECT * FROM hunanyitihua.all_zh WHERE `name` = #{name} AND finalUuid is not null
  </select>
</mapper>